{
  "name": "piotrkwiecinski.com",
  "version": "0.1.0",
  "description": "Source code for piotrkwiecinski.com website",
  "main": "n/a",
  "repository": "git@github.com:piotrkwiecinski/piotrkwiecinski.com.git",
  "author": "Piotr Kwiecinski <piotr.kwiecinski@codemanufacture.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "gatsby": "^2.3.25",
    "gatsby-plugin-google-tagmanager": "^2.0.13",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-robots-txt": "^1.4.0",
    "gatsby-plugin-typescript": "^2.0.12",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0"
  },
  "scripts": {
    "build": "gatsby build",
    "ci-check": "yarn run:parallel lint:* nit:*",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "format:markdown": "prettier --write '{./content/**/*.md,./README.md}'",
    "format:source": "prettier --write 'src/**/*.{ts,tsx}'",
    "lint:source": "tslint './src/**/*.{ts,tsx}'",
    "nit:markdown": "prettier --list-different '{./content/**/*.md,./README.md}'",
    "nit:source": "prettier --list-different 'src/**/*.{ts,tsx}'",
    "run:parallel": "npm-run-all --parallel",
    "serve": "gatsby serve",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@types/node": "^11.13.6",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.8",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.4"
  }
}
