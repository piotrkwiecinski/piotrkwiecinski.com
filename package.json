{
  "name": "piotrkwiecinski.com",
  "version": "0.1.0",
  "description": "Source code for piotrkwiecinski.com website",
  "main": "n/a",
  "repository": "git@github.com:piotrkwiecinski/piotrkwiecinski.com.git",
  "author": "Piotr Kwiecinski <piotr.kwiecinski@codemanufacture.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "gatsby": "^5.14.6",
    "gatsby-plugin-canonical-urls": "^5.14.0",
    "gatsby-plugin-google-tagmanager": "^5.14.0",
    "gatsby-plugin-robots-txt": "^1.8.0",
    "gatsby-plugin-sitemap": "^6.14.0",
    "gatsby-plugin-typescript": "^5.14.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "scripts": {
    "build": "gatsby build",
    "ci-check": "yarn run:parallel lint:* nit:*",
    "clean": "gatsby clean",
    "deploy": "gatsby-plugin-s3 deploy",
    "develop": "gatsby develop",
    "format:markdown": "prettier --write '{content/**/*.md,README.md}'",
    "format:source": "prettier --write 'src/**/*.{ts,tsx}'",
    "lint:source": "tslint 'src/**/*.{ts,tsx}'",
    "nit:markdown": "prettier --list-different '{content/**/*.md,README.md}'",
    "nit:source": "prettier --list-different 'src/**/*.{ts,tsx}'",
    "run:parallel": "npm-run-all --parallel",
    "serve": "gatsby serve",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/node": "^16.18.126",
    "@types/react": "^17.0.88",
    "@types/react-dom": "^17.0.26",
    "@types/react-helmet": "^6.1.11",
    "gatsby-plugin-s3": "^0.4.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.9.2"
  }
}
