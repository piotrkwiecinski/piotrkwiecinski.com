{
  "name": "piotrkwiecinski.com",
  "version": "0.1.0",
  "description": "Source code for piotrkwiecinski.com website",
  "main": "n/a",
  "repository": "git@github.com:piotrkwiecinski/piotrkwiecinski.com.git",
  "author": "Piotr Kwiecinski <piotr.kwiecinski@codemanufacture.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "gatsby": "^2.15.36",
    "gatsby-plugin-canonical-urls": "^2.1.11",
    "gatsby-plugin-google-tagmanager": "^2.1.13",
    "gatsby-plugin-react-helmet": "^3.1.11",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sitemap": "^2.2.17",
    "gatsby-plugin-typescript": "^2.1.12",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-helmet": "^6.0.0-beta"
  },
  "scripts": {
    "build": "gatsby build",
    "ci-check": "yarn run:parallel lint:* nit:*",
    "clean": "gatsby clean",
    "deploy": "gatsby-plugin-s3 deploy",
    "develop": "gatsby develop",
    "format:markdown": "prettier --write '{./content/**/*.md,./README.md}'",
    "format:source": "prettier --write 'src/**/*.{ts,tsx}'",
    "lint:source": "tslint './src/**/*.{ts,tsx}'",
    "nit:markdown": "prettier --list-different '{./content/**/*.md,./README.md}'",
    "nit:source": "prettier --list-different 'src/**/*.{ts,tsx}'",
    "run:parallel": "npm-run-all --parallel",
    "serve": "gatsby serve",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/node": "^11.13.22",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.1",
    "@types/react-helmet": "^5.0.11",
    "gatsby-plugin-s3": "^0.3.2",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.4"
  }
}
