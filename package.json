{
  "name": "piotrkwiecinski.com",
  "version": "0.1.0",
  "description": "Source code for piotrkwiecinski.com website",
  "main": "n/a",
  "repository": "git@github.com:piotrkwiecinski/piotrkwiecinski.com.git",
  "author": "Piotr Kwiecinski <piotr.kwiecinski@codemanufacture.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "gatsby": "^2.32.13",
    "gatsby-plugin-canonical-urls": "^2.10.0",
    "gatsby-plugin-google-tagmanager": "^2.11.0",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-robots-txt": "^1.6.2",
    "gatsby-plugin-sitemap": "^2.12.0",
    "gatsby-plugin-typescript": "^2.12.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0"
  },
  "scripts": {
    "build": "gatsby build",
    "ci-check": "yarn run:parallel lint:* nit:*",
    "clean": "gatsby clean",
    "deploy": "gatsby-plugin-s3 deploy",
    "develop": "gatsby develop",
    "format:markdown": "prettier --write '{content/**/*.md,README.md}'",
    "format:source": "prettier --write 'src/**/*.{ts,tsx}'",
    "lint:source": "tslint 'src/**/*.{ts,tsx}'",
    "nit:markdown": "prettier --list-different '{content/**/*.md,README.md}'",
    "nit:source": "prettier --list-different 'src/**/*.{ts,tsx}'",
    "run:parallel": "npm-run-all --parallel",
    "serve": "gatsby serve",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/node": "^14.17.1",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "@types/react-helmet": "^6.1.1",
    "gatsby-plugin-s3": "^0.3.8",
    "husky": "^4.3.8",
    "lint-staged": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.2"
  }
}
