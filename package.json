{
  "name": "piotrkwiecinski.com",
  "version": "0.1.0",
  "description": "Source code for piotrkwiecinski.com website",
  "main": "n/a",
  "repository": "git@github.com:piotrkwiecinski/piotrkwiecinski.com.git",
  "author": "Piotr Kwiecinski <piotr.kwiecinski@codemanufacture.com>",
  "license": "MIT",
  "private": true,
  "resolutions": {
    "lodash": "4.17.14"
  },
  "dependencies": {
    "gatsby": "^2.13.83",
    "gatsby-plugin-canonical-urls": "^2.1.5",
    "gatsby-plugin-google-tagmanager": "^2.1.7",
    "gatsby-plugin-react-helmet": "^3.1.5",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sitemap": "^2.2.9",
    "gatsby-plugin-typescript": "^2.1.5",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1"
  },
  "scripts": {
    "build": "gatsby build",
    "ci-check": "yarn run:parallel lint:* nit:*",
    "clean": "gatsby clean",
    "deploy": "gatsby-plugin-s3 deploy",
    "develop": "gatsby develop",
    "format:markdown": "prettier --write '{./content/**/*.md,./README.md}'",
    "format:source": "prettier --write 'src/**/*.{ts,tsx}'",
    "lint:source": "tslint './src/**/*.{ts,tsx}'",
    "nit:markdown": "prettier --list-different '{./content/**/*.md,./README.md}'",
    "nit:source": "prettier --list-different 'src/**/*.{ts,tsx}'",
    "run:parallel": "npm-run-all --parallel",
    "serve": "gatsby serve",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@types/node": "^11.13.20",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-helmet": "^5.0.9",
    "gatsby-plugin-s3": "^0.3.2",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3"
  }
}
